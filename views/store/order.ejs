        // 판매처 선택이 변경될 때 이벤트 리스너 추가
        storeSelect.addEventListener('change', async function() {
            var selectedStore = this.value;

            // 판매자 선택창 초기화
            staffSelect.innerHTML = '<option value="전체">전체</option>';

            try {
