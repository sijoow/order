    ///단가 증량 함수 
    function updateItemPrice(row) {
        const pricePerItem = parseInt(row.find('td:nth-child(2) select option:selected').data('price') || 0);
        const quantity = parseInt(row.find('.counter').val());
        const discountOption = row.find('td:nth-child(6) select option:selected').text();
        let discountRate = 0;

        switch(discountOption) {
            case "3%":
                discountRate = 0.03;
                break;                     
            case "5%":
                discountRate = 0.05;
                break;              
            case "10%":
                discountRate = 0.10;
                break;
            case "15%":
                discountRate = 0.15;
                break;
            case "20%":
                discountRate = 0.20;
                break;
                case "25%":
                discountRate = 0.25;
                break;                   
        }
        const totalCostForItem = pricePerItem * quantity * (1 - discountRate);
        const formattedCost = totalCostForItem.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        row.find('td:nth-child(7) input').val(formattedCost);
    }
    function updateTotalPrice() {
        let total = 0;
        $('table tbody tr').each(function() {
            const priceValue = $(this).find('td:nth-child(7) input').val() || "0";
            const price = parseInt(priceValue.toString().replace(/,/g, ''));
            const shippingCost = parseInt($(this).find('td:nth-child(5) select option:selected').data('shipping-cost') || 0);
            total += (price + shippingCost);
        });

        const formattedTotal = total.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        $('#totalPrice').val(formattedTotal);
    }

    $('table tbody').on('change', 'select, input[type="number"]', function() {
        const row = $(this).closest('tr');
        updateItemPrice(row);
        updateTotalPrice();
    });



