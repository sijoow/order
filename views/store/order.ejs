    }
    function decreaseValue(inputElement) {
        let value = parseInt(inputElement.value, 10);
        value = isNaN(value) ? 0 : value;
        if(value > 1) value--;
        inputElement.value = value;

        const row = $(inputElement).closest('tr');
        updateItemPrice(row);
        updateTotalPrice();
